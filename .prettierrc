{
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "tabWidth": 4,
    "singleQuote": true,
    "singleAttributePerLine": true,
    "trailingComma": "all",
    "importOrder": [
        "^@/@enums",
        "^@/@interfaces",
        "^@/@types",
        "^@/stores",
        "^@/composable",
        "^@/utils",
        "^@/router/(.*)$",
        "^@/styles/(.*)$",
        "^@/views/(.vue*)$",
        "^@/components",
        "^[./]"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}
